{
  "name": "minv",
  "version": "0.1.0",
  "license": "GPL-2.0",
  "main": "lua/minv",
  "types": "lua/minv.d.ts",
  "exports": {
    ".": {
      "types": "./lua/minv.d.ts",
      "default": "./lua/minv"
    },
    "./*": {
      "types": "./lua/minv/*.d.ts",
      "default": "./lua/minv/*"
    },
    "./types": {
      "types": "./src/global.d.ts"
    }
  },
  "files": [
    "lua/*",
    "src/global.d.ts"
  ],
  "scripts": {
    "fmt": "prettier --write .",
    "build": "tstl"
  },
  "keywords": [],
  "devDependencies": {
    "prettier": "~2.7.1",
    "typescript": "~4.8.3",
    "typescript-to-lua": "~1.10.1"
  },
  "dependencies": {
    "lua-types": "~2.13.0",
    "spark": "github:loichyan/spark.nvim#dev"
  }
}
